import { SAVE_TEXT_OPTIONS, UPDATE_TEXT_POSITION } from '../contants/actionTypes';

export function saveTextOptions(text, fontStyle, fontSize, fontColor) {
  return {
    type: SAVE_TEXT_OPTIONS,
    text,
    fontStyle,
    fontSize,
    fontColor
  };
}

export function updateTextPosition(id,left,top){
  return {
    type: UPDATE_TEXT_POSITION,
    left,
    top
  }
}
