import React, { Component, PropTypes } from 'react';

export default class XDrag extends Component {
  constructor(props) {
    super(props);
  }

  render() {
    const { children, onDragStarted } = this.props;
    return (
      <div onDragStart={ onDragStarted }
           draggable="true">
        { children }
      </div>
    )
  }
}

XDrag.propTypes = {
  onDragStarted: PropTypes.func.isRequired
}
