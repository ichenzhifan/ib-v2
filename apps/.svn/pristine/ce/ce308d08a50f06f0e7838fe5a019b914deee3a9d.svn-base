import { SAVE_TEXT_OPTIONS } from '../contants/actionTypes';

const textOptions = (state = [], action) => {
  switch (action.type) {
    //保存文字options
    case SAVE_TEXT_OPTIONS: {
      return [...state, {
        text: action.text,
        fontFamily: action.fontFamily,
        fontStyle: action.fontStyle,
        fontSize: action.fontSize,
        fontColor: action.fontColor
      }];
    }
    default:
      return state;
  }
};

export default textOptions;
