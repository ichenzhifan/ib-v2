import { get } from 'lodash';
import { API_SUCCESS } from '../contants/actionTypes';
import { GET_ENV } from '../contants/apiUrl';
import x2jsInstance from '../../../common/utils/xml2js';

/**
 * cookies的reducer, 把新获取的cookie更新到store.
 * @param state
 * @param action
 */
export const urls = (state = {}, action) => {
  switch (action.type) {
    case API_SUCCESS:
      if (action.api.includes(GET_ENV)) {
        const result = x2jsInstance.xml2js(action.response);

        return result.env || {};
      }
      return state;
    default:
      return state;
  }
};


