{
  "ecmaVersion": 7,
  "libs": [
    "browser"
  ],
  "loadEagerly": [
    "imagebox/src/**/*.js"
  ],
  "dontLoad": [
    "node_modules/**/*.js"
  ],
  "plugins": {
    "complete_strings": {
      "maxLength": 15
    },
    "doc_comment": {
      "fullDocs": true,
      "strong": false
    },
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "modules": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "es_modules": {}
  }
}
