.mod-header {
  width: 100%;
  height: 50px;
  overflow:hidden;
  background: #393939;
  background-size: auto 15px;
}

.logo{
  float:left;
  width:83px;
  height:15px;
  //background: url("new-logo-white.svg") no-repeat;
  background-size: auto 15px;
  margin:20px 20px 0 40px;
  cursor:pointer;
}

.logo img{
  height:100%;
}

.project-title{
  display: inline-block;
  margin-top: 10px;
  height: 40px;
  line-height: 40px;
  color: #fff;
  font-size: 12px;
}

.head-nav{
  float:right;
  height:48px;
  margin-top:2px;
}

.nav-item{
  margin-right:40px;
  display: inline-block;
  color: #fff;
  font-size: 12px;
  font-weight: 500;
  cursor: pointer;
  line-height: 48px;
}
