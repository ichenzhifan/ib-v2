.work-space {
  margin-top: 30px;
  width: 1200px;
  transform: translateX(100px);

  .image-editor {
    background-color: #efefef;
    height: 800px;
  }

  .btn-list {
    text-align: center;
    margin: 10px;

    .btn {
      border: none;
      outline: none;
      padding: 0 12px;
      border-radius: 4px;
      background: #000;
      box-shadow: 0 0 2px 2px #fff;
      color: #fff;
      height: 28px;
      line-height: 28px;
      font-size: 14px;

      & + .btn {
        margin-left: 10px;
      }
    }
  }
  p {
    margin: 0;
  }
}
